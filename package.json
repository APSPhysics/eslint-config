  "homepage": "https://github.com/APSPhysics/eslint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
