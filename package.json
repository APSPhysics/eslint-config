{
  "name": "@apsphysics/eslint-config",
  "version": "1.0.0",
  "description": "ESLint configuration for APS projects.",
  "author": "APSPhysics",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write *.js",
    "lint": "eslint",
    "test": "npm run lint && jest",
    "prerelease": "npm run test && npm run format",
    "release": "npm run prerelease && np"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/APSPhysics/eslint-config.git"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0 || >=14.0.0 || >=16.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/APSPhysics/eslint-config/issues"
  },
  "homepage": "https://github.com/APSPhysics/eslint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.6",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-functional": "^5.0.7",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-standard": "^4.1.0"
  },
  "peerDependencies": {
    "eslint": "^7 || ^8",
    "prettier": ">=2.4.1",
    "typescript": ">=4.0.0"
