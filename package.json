{
  "name": "@apsphysics/eslint-config",
  "version": "1.1.1-1",
  "description": "ESLint configuration for APS Physics projects.",
  "author": "APSPhysics",
  "main": "index.js",
  "files": [
    "index.js",
    "eslint-local-rules.js",
    "rules/*.js",
    "node_modules/eslint-plugin-local-rules/**/*"
  ],
  "scripts": {
    "format": "prettier --write *.js",
    "lint": "eslint",
    "test": "npm run lint && jest",
    "prerelease": "npm run test && npm run format",
    "release": "npm run prerelease && np --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/APSPhysics/eslint-config.git"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0 || >=14.0.0 || >=16.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/APSPhysics/eslint-config/issues"
  },
  "homepage": "https://github.com/APSPhysics/eslint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-local-rules": "^1.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0"
  },
  "peerDependencies": {
    "eslint": "^7 || ^8",
    "prettier": ">=2.4.1",
    "typescript": ">=4.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
