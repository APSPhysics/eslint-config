{
  "name": "@apsphysics/eslint-config",
  "version": "0.0.22",
  "description": "ESLint configuration for APS Physics React/TypeScript projects.",
  "author": "APSPhysics",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write *.js",
    "lint": "eslint",
    "test": "npm run lint",
    "prerelease": "npm run test && npm run format",
    "release": "npm run prerelease && np --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/APSPhysics/eslint-config.git"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0 || >=14.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/APSPhysics/eslint-config/issues"
  },
  "homepage": "https://github.com/APSPhysics/eslint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "eslint": "^8.1.0",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "typescript": "^4.5.2"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "react",
    "typescript",
    "styleguide",
    "aps physics"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-immutable": ">=1.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0"
  },
  "peerDependencies": {
    "eslint": "^7.0.0 || ^8.0.0",
    "prettier": ">=2.4.1",
    "typescript": ">=4.5.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
