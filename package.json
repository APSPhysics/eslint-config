{
  "name": "@apsphysics/eslint-config",
  "version": "0.0.20",
  "description": "ESLint configuration for APS Physics React/TypeScript projects.",
  "author": "APSPhysics",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write *.js",
    "lint": "eslint",
    "test": "npm run lint",
    "prerelease": "npm run test && npm run format",
    "release": "npm run prerelease && np --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/APSPhysics/eslint-config.git"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0 || >=14.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/APSPhysics/eslint-config/issues"
  },
  "homepage": "https://github.com/APSPhysics/eslint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "eslint": "^8.1.0",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "react",
    "typescript",
    "styleguide",
    "aps physics"
  ],
  "dependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
